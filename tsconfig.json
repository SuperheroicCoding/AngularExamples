{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "dom",
      "es2018"
    ],
    "baseUrl": "./",
    "paths": {
      "sc-thanos": [
        "dist/sc-thanos"
      ],
      "sc-thanos/*": [
        "dist/sc-thanos/*"
      ]
    }
  }
}
