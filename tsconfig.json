{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strictBindCallApply": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "dom",
      "es2018"
    ],
    "baseUrl": "./",
    "paths": {
      "@fib-wasm": [
        "projects/fib-wasm"
      ],
      "@fib-wasm/*": [
        "projects/fib-wasm/*"
      ],
      "test-lib": [
        "dist/test-lib/test-lib",
        "dist/test-lib"
      ]
    }
  }
}