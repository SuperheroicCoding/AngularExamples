{
  "name": "angular-examples",
  "version": "1.0.32",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run asbuild && ng build sc-thanos && ng build angular-examples",
    "build-thanos": "ng build sc-thanos",
    "test-thanos": "ng test sc-thanos",
    "test": "ng test angular-examples",
    "lint": "ng lint",
    "lint-ci": "ng lint  --format=checkstyle > lintResult.xml",
    "e2e": "ng e2e",
    "asbuild": "asc projects/angular-examples/src/assets/wasm/fib.wasm.as -o projects/angular-examples/src/assets/wasm/fib.wasm -O3 --validate --noAssert --importMemory --sourceMap --measure",
    "ghPages": "npm run asbuild && ng build --prod --output-path docs --named-chunks --base-href https://superheroiccoding.github.io/AngularExamples/",
    "buildS3": "ng build --prod --output-path distS3 --base-href https://angularexamples.superheroiccoding.de",
    "updateFirebase": "node node-scripts/init-firebase",
    "deploy": "npm version patch && npm run asbuild && npm run build-thanos && npm run buildS3 && node node-scripts/sync-S3",
    "deploy-without-build": "node node-scripts/sync-S3",
    "deploy-exclude-assets": "npm version patch && npm run asbuild && npm run build-thanos && npm run buildS3 && node node-scripts/sync-S3 --excludeAssets",
    "fix-memory-limit": "cross-env LIMIT=3096 increase-memory-limit"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.14",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/fire": "^5.1.2",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "7.2.14",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "7.2.14",
    "@angular/service-worker": "7.2.14",
    "@datorama/akita": "^3.10.2",
    "@tensorflow/tfjs": "1.1.2",
    "ace-builds": "^1.4.4",
    "angulartics2": "^7.5.2",
    "core-js": "2.6.5",
    "firebase": " 5.9.4",
    "gl-matrix": "2.8.1",
    "gpu.js": "1.10.4",
    "hammerjs": "^2.0.8",
    "mathjs": "^5.9.0",
    "ng2-ace-editor": "0.3.9",
    "ngx-take-until-destroy": "^5.3.1",
    "p5": "^0.8.0",
